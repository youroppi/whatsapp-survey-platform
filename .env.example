# Environment Variables for WhatsApp Survey Platform

# Server Configuration
PORT=3000
NODE_ENV=development
SESSION_SECRET=your-session-secret-here

# Database Configuration (PostgreSQL)
DATABASE_URL=postgresql://username:password@localhost:5432/whatsapp_survey_db

# WhatsApp Configuration
WHATSAPP_SESSION_NAME=whatsapp-survey-session

# OpenAI Configuration (Required for Voice Transcription)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Voice Processing Configuration
MAX_VOICE_DURATION_SECONDS=60
VOICE_PROCESSING_TIMEOUT_MS=30000
